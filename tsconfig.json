{
  "compilerOptions": {
    "target": "es6",
    // "target": "es2019",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    // "noImplicitAny": true,
    // "esModuleInterop": true,
    "sourceMap": false,
    "outDir": "./dist_2",
    "rootDir": "./",
    "baseUrl": "./",
    "incremental": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["client", "server"],
  "exclude": ["node_modules", "dist", "test", "**/*.spec.ts", "webpack"]
  // "include": [
  //   "./src/front"
  // ]
}
