{
  "name": "sample_nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build:front": "rimraf dist && webpack --config webpack/front/webpack.dev.js",
    "start:front": "webpack-dev-server --config webpack/front/webpack.dev.js",
    "start:dev": "rimraf dist && webpack --config webpack/server/webpack.dev.js",
    "origin:start": "nest start",
    "origin:start:dev": "nest start --watch",
    "origin:start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:base": "node mongodb/insert-sample.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.15",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-history-api-fallback": "^1.6.0",
    "hbs": "^4.1.1",
    "joi": "^17.4.0",
    "mongoose": "^5.12.0",
    "mongoose-autopopulate": "^0.12.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/connect-history-api-fallback": "^1.3.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/joi": "^17.2.3",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.3",
    "@types/supertest": "^2.0.10",
    "@types/webpack": "4.41.0",
    "@types/webpack-dev-middleware": "2.0.3",
    "@types/webpack-hot-middleware": "2.16.5",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "merge": "1.2.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mongodb": "^3.6.5",
    "nodemon-webpack-plugin": "4.2.2",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "sass-loader": "^11.0.1",
    "source-map": "^0.7.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
